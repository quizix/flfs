<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dxw.common.models.InStyPlan" table="flfs_in_sty_plan">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="createTime" column="createTime" not-null="true"/>
        <property name="modifyTime" column="modifyTime" not-null="true"/>

        <property name="date" column="date" not-null="true"/>
        <property name="value" column="value" not-null="true"/>
        <many-to-one name="sty" column="styId"
                     class="com.dxw.common.models.Sty" not-null="true">
        </many-to-one>
    </class>
</hibernate-mapping>
