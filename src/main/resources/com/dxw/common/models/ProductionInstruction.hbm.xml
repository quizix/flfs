<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dxw.common.models.ProductionInstruction" table="flfs_production_instruction">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="createTime" column="createTime"/>
        <property name="modifyTime" column="modifyTime"/>
        <property name="water" column="water"/>
        <property name="dry" column="dry"/>
        <property name="bacteria" column="bacteria"/>
        <property name="barrel1" column="barrel1"/>
        <property name="barrel2" column="barrel2"/>
        <property name="barrel3" column="barrel3"/>
        <property name="barrel4" column="barrel4"/>
        <property name="barrel5" column="barrel5"/>
        <property name="barrel6" column="barrel6"/>

        <many-to-one name="shed" column="shedId"
                     class="com.dxw.common.models.Shed" not-null="true">
        </many-to-one>
    </class>
</hibernate-mapping>