<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping">
<hibernate-mapping>
    <class name="com.dxw.common.models.Sty" table="flfs_sty">
        <id name="id" column="id">
            <generator class="native" />
        </id>
        <property name="createTime" column="createTime" />
        <property name="modifyTime" column="modifyTime" />
        <property name="code" column="code" />
        <property name="no" column="no" />
        <property name="name" column="name" />
                
        <property name="pigNumber" column="pigNumber" />
        <many-to-one name="shed" column="shedId" 
                             class="com.dxw.common.models.Shed" not-null="true">
        </many-to-one>
    </class>
</hibernate-mapping>