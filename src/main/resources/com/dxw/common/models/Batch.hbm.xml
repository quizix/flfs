<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dxw.common.models.Batch" table="flfs_batch">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="createTime" column="createTime"/>
        <property name="modifyTime" column="modifyTime"/>

        <property name="code" column="code"/>
        <property name="inStockDate" column="inStockDate"/>
        <property name="inStockDuration" column="inStockDuration"/>
        <property name="inStockAmount" column="inStockAmount"/>

        <set name="sties"
             table="flfs_batch_sty"
             cascade="save-update">
            <key column="batchId"/>
            <many-to-many class="com.dxw.common.models.Sty" column="styId"/>
        </set>


    </class>
</hibernate-mapping>